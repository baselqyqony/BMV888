@model IEnumerable<Bee_Project.Areas.ServiceProvider.Models.ServiceAppointmentItem>

@{
    ViewBag.Title = "serviceUserAppiointments";
   // Layout = "~/Areas/ServiceProvider/Views/Shared/_Layout.cshtml";
}

<h2>service UserAppiointments</h2>


<table class="table">
    <tr>
        <th>
            Date
        </th>

        <th>
          User Name
        </th>

        <th>
           startTime
        </th>

        <th>
            EndTime
        </th>

        <th>
            Email
        </th>

        <th>
            Phone
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UAppointment.startTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UAppointment.endTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.phone)
        </td>
    </tr>
}

</table>
